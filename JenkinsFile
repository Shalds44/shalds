pipeline {
    agent any
    stages {
        stage('Clone') {
            steps{
                git 'https://github.com/Shalds44/shalds'
            }
        }
        stage('Build') {
            steps {
                sh 'node --version'
                sh 'svn --version'
            }
        }
    }
}

