pipeline {
    agent { dockerfile true }
    stages {
        stage("path"){
            steps{
                sh 'echo $PATH'
            }
        }
        stage('Clone') {
            steps{
                git 'https://github.com/Shalds44/shalds'
            }
        }
    }
}

